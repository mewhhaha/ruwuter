{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable",
      "DOM.AsyncIterable"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "moduleDetection": "force",
    "jsx": "react-jsx",
    "jsxImportSource": "@mewhhaha/ruwuter",
    "allowImportingTsExtensions": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "verbatimModuleSyntax": true,
    "strict": true,
    "skipLibCheck": true,
    "erasableSyntaxOnly": true,
    "resolveJsonModule": true,
    "paths": {
      "@mewhhaha/ruwuter/jsx-runtime": [
        "./src/runtime/jsx-runtime.mts"
      ],
      "@mewhhaha/ruwuter/jsx-dev-runtime": [
        "./src/runtime/jsx-dev-runtime.mts"
      ]
    }
  },
  "include": [
    "src",
    "test",
    "test-dom"
  ],
  "files": [
    "src/global.workers.d.ts"
  ]
}
